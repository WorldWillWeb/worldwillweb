{% extends 'default.html.twig' %}

{% block content_wrapper %}
    {% block jumbo_wrapper %}
        <header class="md:flex bg-header-blue py-10 items-center">
            <div class="md:flex w-2/3 mx-auto">
            <div class="md:flex-shrink-0 px-20 mb-10 md:mb-0 md:w-1/2">
                <img class="rounded-b-full md:w-56 w-32 mx-auto p-6" src="{{ site.url }}/build/jackson/512x512.png" alt="Jackson posing for a photograph">
            </div>
            <div class="mt-4 md:mt-0 md:ml-6 md:w-1/2">
                <div class="uppercase tracking-wide text-2xl text-blue-900 font-bold">{{ site.title }}</div>
                <p class="block mt-1 text-xl leading-tight font-semibold text-yellow-400 leading-relaxed">{{ page.page_title }}</p>
                <hr>
                <p class="mt-2 text-lg text-white leading-snug">{{ page.page_subtitle }}</p>
            </div>
            </div>
        </header>
    {% endblock jumbo_wrapper %}

    {% block content '' %}
{% endblock %}


